{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/components/*"],
      "#/*": ["src/components/ui/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
